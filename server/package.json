{
  "name": "hmm-server",
  "version": "1.0.0",
  "description": "News collection and analysis",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "esbuild": "esbuild src/getNewsPiecesHandler.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/getNewsPiecesHandler.js",
    "start": "npx tsc && node ./build/index.js",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run start\"",
    "test": "jest",
    "samBuild": "sam build --template-file getNewsPiecesBackend.yaml --build-dir ./dist",
    "deploy": "sam deploy --template-file getNewsPiecesBackend.yaml --stack-name HmmmIsThatRight --capabilities CAPABILITY_IAM CAPABILITY_NAMED_IAM --s3-bucket hmmm-dev-storage-bucket --region eu-west-2"
  },
  "keywords": [
    "news",
    "analysis",
    "sentiment",
    "accuracy"
  ],
  "author": "Orion",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/express": "^4.17.9",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^29.5.4",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.9",
    "jest": "^29.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^29.1.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.5.0",
    "archiver": "^5.3.1",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "esbuild": "^0.17.18",
    "express": "^4.17.1",
    "express-pino-logger": "^7.0.0",
    "google-search-results-nodejs": "^2.0.1",
    "http-status-codes": "^2.2.0",
    "jsdom": "^16.4.0",
    "pino": "^7.9.2",
    "pino-pretty": "^7.6.0"
  }
}
