{
  "name": "hmmm-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "axios": "^1.7.9",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^6.30.3",
    "sort-by": "^1.2.0",
    "styled-components": "^6.3.9"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run",
    "test:integration": "playwright test tests/integration",
    "test:integration:debug": "playwright test tests/integration --debug --project=chromium",
    "test:integration:ui": "playwright test tests/integration --ui",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:debug": "playwright test tests/e2e --debug --project=chromium",
    "test:e2e:ui": "playwright test tests/e2e --ui",
    "test:create": "playwright codegen http://localhost:3000"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.19.11",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.2.1",
    "jsdom": "^26.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.2.5",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.1",
    "eslint": "^8.47.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
